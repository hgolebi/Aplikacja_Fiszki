            <MediaView fitHeight="50.0" fitWidth="50.0">
               <mediaPlayer>
                  <MediaPlayer fx:id="player">
                     <media>
                        <Media source="@../water.mp3" />
                     </media>
                  </MediaPlayer>
               </mediaPlayer>
            </MediaView>
         </children></VBox>
   </children>
