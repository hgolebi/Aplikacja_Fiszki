                        </children>
                     </VBox>
                  </graphic>
               </ToggleButton>
               <Button fx:id="right_swipe_button" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#OnRightSwipeClicked" prefHeight="300.0" text="&gt;" visible="false" HBox.hgrow="NEVER">
