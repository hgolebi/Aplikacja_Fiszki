                     <VBox alignment="CENTER" styleClass="fiszka" stylesheets="@../fish.css">
                        <children>
                           <Label fx:id="term_label" text="SŁOWO" textAlignment="CENTER">
                              <font>
                                 <Font size="50.0" />
                              </font>
                           </Label>
                           <Label fx:id="category_label" text="kategoria" textAlignment="CENTER">
