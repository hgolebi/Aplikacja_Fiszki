               <Font name="System Bold" size="18.0" />
            </font>
         </Button>
         <HBox alignment="CENTER" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" VBox.vgrow="ALWAYS">
            <children>
               <Button fx:id="left_swipe_button" alignment="CENTER" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#OnLeftSwipeClicked" prefHeight="300.0" text="&lt;" visible="false">
